[Nemo Action]
Name=Edit with Emacs
Comment=Edit the selected file(s) with the Emacs Text Editor
Quote=double
Exec=emacsclient --alternate-editor="" -c %F
Selection = any
Extensions=bash;c;c++;coffee;conf;cpp;css;dart;go;h;h++;hpp;htm;html;ini;jade;js;log;lua;md;me;opa;pl;py;rb;rc;sh;sql;ts;txt;yaml
